{
    "name": "khelaia/pixover",
    "description": "image optimization while uploading",
    "license": "MIT",
    "authors": [
        {
            "name": "Dimitri Khelaia",
            "email": "ditokhelaia@gmail.com",
            "homepage": "https://github.com/khelaia/",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/khelaia/pixover",
    "keywords": ["Laravel", "pixover","image","upload","lightweight"],
    "require": {
        "illuminate/support": "~5|~6",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "khelaia\\pixover\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "khelaia\\pixover\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "khelaia\\pixover\\pixoverServiceProvider"
            ],
            "aliases": {
                "pixover": "khelaia\\pixover\\Facades\\pixover"
            }
        }
    }
}
